int : n = 3;
int : m = 3;

set of int : P = 1..n;
set of int : M = 1..m;

%-----------------------------------------------------------------------
% ∀i ∈ P,Vi = {vi1,..., vim}
% ∀i ∈ P,∀ j ∈ [1,m],D(vi j) = {1,...,m}
%-----------------------------------------------------------------------
array [P,M] of var M : V;

%-----------------------------------------------------------------------
% ∀i ∈ P, all different(vi1,..., vim)
%-----------------------------------------------------------------------
include "globals.mzn";
constraint
	forall(i in P)(
		all_different(j in M)(V[i,j]));

array[P,M,1..m-1] of var bool : d;
constraint
	